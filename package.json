{
  "name": "@whppt/form-builder",
  "version": "2.3.7",
  "description": "Whppt form builder modules for use with the nuxt framework",
  "repository": "https://github.com/whpptjs/form-builder",
  "license": "MIT",
  "contributors": [
    {
      "name": "lucas <info@sveltestudios.com>"
    }
  ],
  "files": [
    "lib"
  ],
  "main": "lib/module.js",
  "scripts": {
    "dev": "nuxt example",
    "build": "nuxt build example",
    "start": "nuxt start example",
    "lint": "eslint --ext .js,.vue .",
    "release:patch": "yarn test && standard-version --release-as patch && yarn publish && git push --follow-tags",
    "release:minor": "yarn test && standard-version --release-as minor && yarn publish && git push --follow-tags",
    "release:major": "yarn test && standard-version --release-as major && yarn publish && git push --follow-tags",
    "test": "yarn lint && jest --passWithNoTests"
  },
  "dependencies": {
    "@nuxtjs/eslint-config": "5.0.0",
    "axios": "^0.27.1",
    "eslint": "^7.0.0",
    "lodash": "^4.17.20",
    "v-calendar": "2.4.1",
    "vue": "^2.6.14",
    "vue-csv-downloader": "^1.0.3",
    "vue-recaptcha": "^2.0.3",
    "vuelidate": "^0.7.7",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-module": "^4.0.1",
    "@nuxtjs/module-test-utils": "latest",
    "babel-eslint": "^10.1.0",
    "babel-jest": "latest",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^8.0.1",
    "husky": "latest",
    "jest": "^28.0.2",
    "lint-staged": "^12.4.1",
    "nuxt-edge": "latest",
    "prettier": "^2.1.2",
    "standard-version": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
